{
  "name": "test-with-redux",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "---------- DEVELOPMENT ---------------------------------------------------------------------------------": "",
    "------RUN-FRONT-BACK-TOGATHER-CMDs------": "",
    "dev": "concurrently \"yarn start:api:dev\" \"yarn start:frontend:dev\"",
    "start": "npx serve -s build",
    "------ALL-DEVELOPMENT-CMDs------": "",
    "start:frontend:dev": "env-cmd -e local_start -r env-cmd.js react-scripts start",
    "start:api:dev": "yarn --cwd backend run start:dev",
    "------ALL-BUILDs-CMDs------": "",
    "build": "react-scripts build",
    "------ALL-LINTING-CMDs------": "",
    "lint:eslint": "yarn lint",
    "lint:ts": "yarn typecheck",
    "lint": "yarn eslint src/**/*.ts src/**/*.tsx",
    "typecheck": "tsc --noEmit",
    "------ALL-FORMATTING-CMDs------": "",
    "format": "prettier --write '**/**.{js,jsx,ts,tsx,json,css,md,html}'"
  },
  "dependencies": {
    "@date-io/date-fns": "1.x",
    "@material-ui/core": "^4.11.2",
    "@material-ui/data-grid": "4.0.0-alpha.22",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/lab": "^4.0.0-alpha.57",
    "@material-ui/pickers": "3.2.10",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "@types/jest": "^26.0.15",
    "@types/node": "^12.0.0",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/uuid": "^8.3.0",
    "classnames": "2.2.6",
    "concurrently": "^5.3.0",
    "connected-react-router": "6.7.0",
    "date-fns": "2.18.0",
    "dayjs": "1.8.21",
    "dotenv": "^8.2.0",
    "env-cmd": "^10.1.0",
    "history": "4.10.1",
    "react": "^17.0.1",
    "react-app-polyfill": "1.0.6",
    "react-cool-tabs": "1.1.6",
    "react-dom": "^17.0.1",
    "react-query": "3.12.0",
    "react-redux": "7.2.0",
    "react-router-dom": "5.1.2",
    "react-scripts": "4.0.2",
    "redux": "4.0.5",
    "redux-devtools-extension": "2.13.8",
    "redux-freeze": "0.1.7",
    "redux-thunk": "2.3.0",
    "semantic-ui-css": "2.4.1",
    "semantic-ui-react": "0.88.2",
    "sjs-base-model": "1.9.0",
    "typescript": "^4.1.2",
    "uuid": "7.0.1",
    "web-vitals": "^1.0.1"
  },
  "devDependencies": {
    "@babel/plugin-proposal-nullish-coalescing-operator": "7.8.3",
    "@babel/plugin-proposal-optional-chaining": "7.8.3",
    "@craco/craco": "5.6.3",
    "@cypress/code-coverage": "^3.9.0",
    "@testing-library/cypress": "^7.0.3",
    "@testing-library/jest-dom": "^5.11.8",
    "@testing-library/react": "^11.2.2",
    "@types/classnames": "2.2.9",
    "@types/history": "4.7.5",
    "@types/jest": "25.1.3",
    "@types/node": "13.7.7",
    "@types/react": "16.9.23",
    "@types/react-dom": "16.9.5",
    "@types/react-redux": "7.1.7",
    "@types/react-router-dom": "5.1.3",
    "@types/redux-mock-store": "1.0.2",
    "@types/uuid": "7.0.0",
    "@typescript-eslint/eslint-plugin": "2.21.0",
    "@typescript-eslint/parser": "2.21.0",
    "cross-env": "7.0.0",
    "eslint": "7.11.0",
    "eslint-config-prettier": "6.10.0",
    "eslint-plugin-cypress": "2.11.2",
    "eslint-plugin-prettier": "3.1.2",
    "eslint-plugin-react": "7.18.3",
    "eslint-plugin-react-hooks": "2.5.0",
    "generate-template-files": "2.2.1",
    "gh-pages": "2.2.0",
    "husky": "4.2.3",
    "lint-staged": "^10.5.3",
    "nock": "12.0.3",
    "node-sass": "4.13.1",
    "prettier": "1.19.1",
    "pretty-quick": "2.0.1",
    "react-scripts": "3.4.0",
    "react-test-renderer": "^17.0.1",
    "redux-mock-store": "1.5.4"
  },
  "resolutions": {
    "react-scripts/eslint-webpack-plugin": "2.3.0"
  },
  "jest": {
    "resetMocks": false
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": "yarn eslint --cache --fix",
    "*.{js,jsx,ts,tsx,json,css,md,html}": "pretty-quick --staged --pattern '**/*.*(js|jsx|ts|tsx|json|css|md|html)'"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
